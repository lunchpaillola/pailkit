# PailKit API Environment Variables
# Copy this file to .env and fill in your actual API keys

# Daily.co API Key
DAILY_API_KEY=your-daily-api-key-here

# Unkey (API access + verification)
# Root key used by the server to verify incoming PailKit keys
UNKEY_ROOT_KEY=your-unkey-root-key-here
# API identifier from Unkey (optional for now, reserved for future use)
UNKEY_API_ID=your-unkey-api-id-here

UNKEY_PAILKIT_SECRET=
