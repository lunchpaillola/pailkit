# Flow Environment Variables
# Copy this file to .env and fill in your actual values

# MealMe API Key
MEALME_API_KEY=your-mealme-api-key-here

# Default Address
DEFAULT_ADDRESS=your-default-address-here

# Unkey (API access + verification)
# Root key used by the server to verify incoming PailKit keys
UNKEY_ROOT_KEY=your-unkey-root-key-here
# API identifier from Unkey (optional for now, reserved for future use)
UNKEY_API_ID=your-unkey-api-id-here
# PailKit secret for Unkey
UNKEY_PAILKIT_SECRET=your-unkey-pailkit-secret-here
