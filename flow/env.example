# Flow Environment Variables
# Copy this file to .env and fill in your actual values

# MealMe API Key
MEALME_API_KEY=your-mealme-api-key-here

# Default Address
DEFAULT_ADDRESS=your-default-address-here

# Unkey (API access + verification)
# Root key used by the server to verify incoming PailKit keys
UNKEY_ROOT_KEY=your-unkey-root-key-here
# API identifier from Unkey (optional for now, reserved for future use)
UNKEY_API_ID=your-unkey-api-id-here
# PailKit secret for Unkey
UNKEY_PAILKIT_SECRET=your-unkey-pailkit-secret-here

# Vapi (Realtime voice assistant)
VAPI_ASSISTANT_ID=your-vapi-assistant-id-here
VAPI_API_KEY=your-vapi-api-key-here
VAPI_PUBLIC_KEY=your-vapi-public-key-here
VAPI_PHONE_NUMBER_ID=your-vapi-phone-number-id-here

# Daily.co Phone Number (for PIN dial-in)
DAILY_PHONE_NUMBER=your-daily-phone-number-here

# Staging Environment
# Set to "DEVELOPMENT" to enable development mode features (e.g., unique room names)
# When set to "DEVELOPMENT", room names will be auto-generated as DEV-DDMMYYYYHHMMSSFFFFFF
# (includes microseconds for guaranteed uniqueness - up to 1 million rooms per second)
STAGING_ENVIRONMENT=
